        "ext-intl": "*",
        "ext-json": "*",
        "ext-libxml": "*",
        "ext-mbstring": "*",
        "ext-mysqli": "*",
        "ext-pcntl": "*",
        "ext-pdo": "*",
        "ext-posix": "*",
        "ext-readline": "*",
        "ext-simplexml": "*",
        "ext-sockets": "*",
        "ext-xml": "*",
        "ext-zend-opcache": "*",
        "ext-zip": "*",
